To find the unique no from an array which is not repeating :

input: 3,2,3,5,6,5,2
output: 6

*******************************************************************************************************************************************************************

#include <iostream>
#include <bits/stdc++.h>
using namespace std ;

int main()
{ 
   int arr[100] , no=0 , n;
   cout<<"Enter the size "<<endl ;
   cin>>n ;
   cout<<"Enter an array"<<endl ;
   for (int i = 0; i < n ; i++)
   {
       cin>>arr[i] ;
       no=no^arr[i] ;
       cout<<no<<endl ;
   }
   cout<<"answer: "<<no ;
    
    return 0;
}
