#include <iostream>
#include <string>
using namespace std;

int main()
{
   int n , arr[100];
   cout<<"Enter the size :"<< endl ;
   cin>> n ;
   cout<<"Enter the elements :"<<endl ;
   for (int i = 0; i < n; i++)
   {
       cin>>arr[i] ;
   }
   
   for(int i = 0 ; i< n ; i++){
       for(int j=i ; j<n ; j++){
           for(int k=i ; k<=j ; k++){
               cout<<arr[k]<<"," ;
           } 
           cout<<endl;
       }
   }

    return 0;
}
