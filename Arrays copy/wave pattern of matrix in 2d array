#include <iostream>
#include <string>
using namespace std;

int main()
{
   int n , arr[100][100] ,m, num ;
   cout<<"Enter the rows"<< endl ;
   cin>> n ;
   cout<<"Enter the columns"<<endl ;
   cin>> m ;
   cout<<"Enter the elements :"<<endl ;
    
   for (int i = 0; i < n; i++)
   {
    for(int j=0 ; j<m ; j++){
       cin>>arr[i][j];
       }
   }
    
    for (int i = 0; i < n; i++)
   {
       if(i%2 == 0){
           for(int j=0 ; j<m ; j++){
               cout<<arr[j][i]<<" " ;
       }
       }
       else{
           for(int j= m-1 ; j>=0 ; j--){
               cout<<arr[j][i]<<" " ;
           }
       }
    //   cout<<endl ;
   }
 
    return 0;
}
